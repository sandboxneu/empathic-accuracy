{
    "version": 2,
    "name": "empathic",
    "builds": [
        { "src": "package.json", "use": "@now/static-build" }
    ],
    "routes": [
      {"src": "^/static/(.*)", "dest": "/static/$1"},
      {"src": "/.*", "dest": "/index.html"}
    ],
    "alias": "empathic.sandboxneu.com",
    "env": {
        "NODE_ENV": "production",
        "REACT_APP_SERVER_URL": "https://api.sandboxneu.com/production-empathic2"
    }
}
